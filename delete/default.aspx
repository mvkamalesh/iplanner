            // page is in edit mode
            $('#EditPageLink').hide();
            $('#Edit_setting').show();
 
        }
        else {
            // page is in browse mode
            $('#EditPageLink').show();
            $('#Edit_setting').hide();
            document.onkeydown = function (event) {
                if (window.event) {
                    e = window.event;
                    var keycode = e.keyCode;
                }
                // keycode for firefox, safari and opera
                else {
                    var keycode = event.which;
                }

                // check Press F2 keycode(firefox, safari and opera) and base on this page goes to edit mode
                if (keycode == 113) {

                    var ctrlKey = detectCTRLKey(event);
                    if (ctrlKey) {
                        $("#cepHelper").toggle();
                    }
                    else {
                        switchToAuthoringMode();
                    }
                }
                else if (keycode == 113) {
                    var ctrlKey = detectCTRLKey(event);
                    if (ctrlKey) {
                        $("#cepHelper").toggle();
                    }
                    else {
                        switchToAuthoringMode();
                    }
                }
            }

        }
