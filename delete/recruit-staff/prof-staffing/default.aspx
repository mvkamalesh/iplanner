                    var ctrlKey = detectCTRLKey(event);
                    if (ctrlKey) {
                        $("#cepHelper").toggle();
                    }
                    else {
                        switchToAuthoringMode();
                    }
                }
                else if (keycode == 113) {
                    var ctrlKey = detectCTRLKey(event);
                    if (ctrlKey) {
                        $("#cepHelper").toggle();
                    }
                    else {
                        switchToAuthoringMode();
                    }
                }
            }

