        <script type="text/javascript">
            function LoadEmailAlertiframe() {
                LoadEmailAlertWindow();
            }

            function LoadEmailAlertWindow() {
                $('#linkEmailAlertiframe').click();
                return false;
            }

            //Transfer focus from close button to the first textbox of Edit Page Settings popup window.

            $('#CloseEditPageSettings').blur(function () {

                $("[id$=_WindowTitleField_ctl00_TextField]").focus();
            });

            ///<summary>This method will be used to show Site setting popup</summary>
            ///<param>none</param>
            ///<return>none</return>

            function ShowEditPageSettingsLayout() {

                var objLightbox = document.getElementById('lightbox');
                var arrayPageSize = getPageSize();

                document.getElementsByTagName('html')[0].style.overflow = 'hidden'; //To disable scroll bar

                $('#pop-up-main-div').css('display', '');
                $("#lightbox, #pop-up-main-div").fadeIn();
                objLightbox.style.height = (arrayPageSize[1] + 'px');

                //Set tabindex of all controls in the Edit page settings pop-up window
                var tabindex = 1;
                $('#pop-up-main-div').find("input[type='text'], textarea, input[type='checkbox']").each(function () {
                    if (this.type != "hidden") {
                        var $input = $(this);
                        $input.attr("tabindex", tabindex);

                        // focus the first one.
                        if (tabindex == 1) {
                            $input.focus();
                        }
                        tabindex++;
                    }
                });
                //Place a canonical tag to canonical textarea when clicking "Edit page settings".
                if ($('[id*=txtCanonical]').text().trim() != "") {
                    $('[id*=txtCanonical]').text($('link[rel="canonical"]').attr('href'));
                }
                

                //Set tabindex of close button
                $('#CloseEditPageSettings').attr("tabindex", tabindex);
            }

            ///<summary>This method will be used to hide Site setting popup</summary>
            ///<param>none</param>
            ///<return>none</return>

            function CloseEditPageSettingsLayout() {
                    document.getElementsByTagName('html')[0].style.overflow = "scroll"; //To enable scroll bar
                    $("#lightbox, #pop-up-main-div").fadeOut(); //It will remove lightbox
                    $("#pop-up-main-div").css('display', 'none'); //
            }

            ///<summary>This method will be used to get current page and window's dimensions viz width and height 
            ///so that background panel will cover complete window</summary>
            ///<param>none</param>
            ///<return type='Array'>This will return array which contain page width,page height,window width and window height</return>

            function getPageSize() {

                var xScroll, yScroll;

                if (window.innerHeight && window.scrollMaxY) {
                    xScroll = document.body.scrollWidth;
                    yScroll = window.innerHeight + window.scrollMaxY;
                } else if (document.body.scrollHeight > document.body.offsetHeight) { // all but Explorer Mac
                    xScroll = document.body.scrollWidth;
                    yScroll = document.body.scrollHeight;
                } else { // Explorer Mac...would also work in Explorer 6 Strict, Mozilla and Safari
                    xScroll = document.body.offsetWidth;
                    yScroll = document.body.offsetHeight;
                }

                var windowWidth, windowHeight;
                if (self.innerHeight) {	// all except Explorer
                    windowWidth = self.innerWidth;
                    windowHeight = self.innerHeight;
                } else if (document.documentElement && document.documentElement.clientHeight) { // Explorer 6 Strict Mode
                    windowWidth = document.documentElement.clientWidth;
                    windowHeight = document.documentElement.clientHeight;
                } else if (document.body) { // other Explorers
                    windowWidth = document.body.clientWidth;
                    windowHeight = document.body.clientHeight;
                }

                // for small pages with total height less then height of the viewport
                if (yScroll < windowHeight) {
                    pageHeight = windowHeight;
                } else {
